{
    "manifest_version": 2,
    "name": "Dreamworks",
    "version": "1.0",
    "description": "Keep track of job openings of the companies you like",
    "icons": {
        "48": "icons/border-48.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "background.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "page": "background.html"
    },
    "browser_action": {
        "default_icon": {
            "19": "button/geo-19.png",
            "38": "button/geo-38.png"
        }
    }
}